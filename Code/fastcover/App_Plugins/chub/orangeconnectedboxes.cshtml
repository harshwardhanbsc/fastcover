@inherits Umbraco.Web.Mvc.UmbracoViewPage<dynamic>
@{
    var hv = Model;
    var tabData = hv["tabelsData"];
}
<div class="servicelistarea">
    @foreach (var row in tabData)
    {
        <div class="servicelistbox">
            <div class="circleicon">
                @if (row.image != null)
                {
                    <img src="@row.image.image" alt="@row.image.altText" />
                }
            </div>
            <h3>@row.heading</h3>
            <div class="chrichtext orangebg">@Html.Raw(row.rteValue)</div>
        </div>
    }
    <div class="clear"></div>
</div>